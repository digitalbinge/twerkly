<p id="notice"><%= notice %></p>

<h1>Twerks</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twerks.each do |twerk| %>
      <tr>
        <td><%= twerk.content %></td>
        <td><%= twerk.user %></td>
        <td><%= link_to 'Show', twerk %></td>
        <td><%= link_to 'Edit', edit_twerk_path(twerk) %></td>
        <td><%= link_to 'Destroy', twerk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twerk', new_twerk_path %>
